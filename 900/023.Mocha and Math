#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }      
        int res=arr[0];
        for(int i=1;i<n;i++)
        {
            res&=arr[i];
        }
        cout<<res<<endl;
    }

}
